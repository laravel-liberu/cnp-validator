        "laravel",
        "laravel-liberu",
        "cnp",
        "cnp-validator",
        "validator"
    ],
    "type": "library",
    "homepage": "https://github.com/laravel-liberu/CnpValidator",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
